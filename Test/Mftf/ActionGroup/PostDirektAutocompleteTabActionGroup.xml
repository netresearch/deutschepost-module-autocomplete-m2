<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">

    <!-- Expand "Autcomplete" tab -->
    <actionGroup name="PostDirektAutocompleteTabActionGroup" extends="ScrollToAndExpandGroupActionGroup">
        <arguments>
            <argument name="groupName" defaultValue="AdminSystemConfigCustomerPostDirektAutocompleteSection.postdirektAutcoompleteGroup"/>
            <argument name="groupNameExpanded" defaultValue="AdminSystemConfigCustomerPostDirektAutocompleteSection.postdirektAutcoompleteGroupExpanded"/>
        </arguments>
    </actionGroup>
</actionGroups>
